package main

import (
	"fmt"
)

func main() {
	var slice1 []string
	fmt.Println(slice1)

	slice2 := make([]string, 3)
	fmt.Println(slice2)

	slice2[0] = "a"
	slice2[1] = "b"
	slice2[2] = "c"
	fmt.Println("set:", slice2)
	fmt.Println("get:", slice2[2])

	fmt.Println("length:", len(slice2))

	slice2 = append(slice2, "d")
	slice2 = append(slice2, "e", "f")
	fmt.Println("append:", slice2)
}
